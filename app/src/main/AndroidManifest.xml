<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="idat.com.minimarketapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MinimarketApp">
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Datos_personales" />
        <activity android:name=".PantallaInicioActivity"></activity>
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@android:style/Theme.DeviceDefault.NoActionBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Carrito" /></activity>

        <activity android:name=".ListadoActivity"></activity>

        <activity android:name=".ComentarioActivity"></activity>

    </application>

</manifest>